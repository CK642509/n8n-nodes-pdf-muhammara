{
  "compilerOptions": {
    "module": "commonjs",
    "target": "ES2019",
    "outDir": "dist",
    "rootDir": ".",
    "declaration": true,
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "moduleResolution": "node"
  },
  "include": [
    "nodes/**/*"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
