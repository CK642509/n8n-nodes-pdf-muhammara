{
  "name": "n8n-nodes-pdf-muhammara",
  "version": "0.1.1",
  "description": "n8n custom node for PDF encryption using muhammara",
  "keywords": [
    "n8n-community-node-package",
    "n8n",
    "pdf",
    "encryption",
    "muhammara"
  ],
  "license": "MIT",
  "homepage": "https://github.com/CK642509/n8n-nodes-pdf-muhammara",
  "author": {
    "name": "CK642509",
    "email": "john830110@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CK642509/n8n-nodes-pdf-muhammara.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint \"nodes/**/*.ts\" package.json",
    "lintfix": "eslint \"nodes/**/*.ts\" package.json --fix",
    "prepublishOnly": "npm run build && npm run lint"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [],
    "nodes": [
      "dist/nodes/PdfEncrypt/PdfEncrypt.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "n8n-workflow": "^1.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "dependencies": {
    "muhammara": "^6.0.0"
  }
}
